<h1>Listing Disasters</h1>
<%= @disasters %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disasters['data'].each do |hash| %>
      <tr>
        <td><%= hash['fields']['name'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Disaster lister', new_disaster_lister_path %>
